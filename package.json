{
  "name": "hacker-news-client-2024",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "engines": {
    "npm": ">=10.8.2",
    "node": ">=20.17.0",
    "yarn": ">=1.22.22"
  },
  "scripts": {
    "dev": "VITE_APP_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g') VITE_GIT_SHA1=$(git show --format=\"%h\" --no-patch) VITE_GIT_BRANCH_NAME=$(git rev-parse --abbrev-ref HEAD) vite --port 3001 --open",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite --port 3000 --open preview",
    "build:prod": "tsc && VITE_APP_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g') VITE_GIT_SHA1=$(git show --format=\"%h\" --no-patch) VITE_GIT_BRANCH_NAME=$(git rev-parse --abbrev-ref HEAD) vite build --mode production --emptyOutDir",
    "_deploy2:staging": "yarn build:prod && bash _aux-tool.deploy2.prod.sh",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.2.1",
    "@mui/material": "^6.2.1",
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "javascript-time-ago": "^2.5.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-redux": "^9.2.0",
    "react-router-dom": "6.x",
    "react-time-ago": "^7.3.3",
    "redux-persist": "^6.0.0",
    "retry-axios": "^3.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^2.1.8",
    "@vitest/web-worker": "^2.1.8",
    "browserslist-to-esbuild": "^2.1.1",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "jsdom": "^25.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.83.0",
    "sass-embedded": "^1.83.0",
    "slugify": "^1.6.6",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "4.5.0",
    "vitest": "^2.1.8"
  }
}
